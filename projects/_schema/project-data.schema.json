{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "project-data.schema.json",
  "title": "Project Data",
  "description": "Schema for project data including tasks, vendors, receipts, and notes",
  "type": "object",
  "required": ["version", "tasks", "vendors", "receipts", "notes"],
  "properties": {
    "$schema": { "type": "string" },
    "version": { "type": "string", "pattern": "^\\d+\\.\\d+$" },
    "tasks": {
      "type": "array",
      "items": { "$ref": "#/$defs/Task" }
    },
    "vendors": {
      "type": "array",
      "items": { "$ref": "#/$defs/Vendor" }
    },
    "receipts": {
      "type": "array",
      "items": { "$ref": "#/$defs/Receipt" }
    },
    "notes": {
      "type": "array",
      "items": { "$ref": "#/$defs/Note" }
    },
    "milestones": {
      "type": "array",
      "items": { "$ref": "#/$defs/Milestone" }
    },
    "payments": {
      "type": "array",
      "items": { "$ref": "#/$defs/Payment" },
      "description": "Owner payments to contractor (deposit, draws, final)"
    },
    "changeOrders": {
      "type": "array",
      "items": { "$ref": "#/$defs/ChangeOrder" },
      "description": "Contract change orders with approval workflow"
    },
    "budget": { "$ref": "#/$defs/Budget" },
    "contract": { "$ref": "#/$defs/Contract" },
    "categories": {
      "type": "array",
      "items": { "type": "string" },
      "description": "Project-specific work categories (extends schema enum)"
    },
    "statuses": {
      "type": "array",
      "items": { "type": "string" },
      "description": "Project-specific status values (extends schema enum)"
    },
    "issues": {
      "type": "array",
      "items": { "$ref": "#/$defs/Issue" },
      "description": "Unified issues system (questions + auto-detected issues)"
    }
  },
  "$defs": {
    "Task": {
      "type": "object",
      "required": ["id", "name"],
      "properties": {
        "id": {
          "type": "string",
          "pattern": "^[a-z0-9-]+$",
          "description": "Unique identifier (slug format)"
        },
        "name": {
          "type": "string",
          "description": "Display name"
        },
        "start": {
          "type": "string",
          "format": "date",
          "description": "Start date YYYY-MM-DD (optional until scheduled)"
        },
        "end": {
          "type": "string",
          "format": "date",
          "description": "End date YYYY-MM-DD (optional until scheduled)"
        },
        "progress": {
          "type": "number",
          "minimum": 0,
          "maximum": 100,
          "description": "Completion percentage"
        },
        "dependencies": {
          "type": "array",
          "items": { "type": "string" },
          "description": "Array of task IDs that must complete first"
        },
        "assignee": {
          "type": "string",
          "pattern": "^vendor:[a-z0-9-]+$",
          "description": "Tag reference to vendor"
        },
        "status": {
          "type": "string",
          "enum": ["pending", "needs-scheduled", "scheduled", "confirmed", "in-progress", "completed", "blocked", "cancelled"],
          "description": "Current status"
        },
        "priority": {
          "type": "string",
          "enum": ["low", "normal", "high", "critical"],
          "description": "Task priority level"
        },
        "category": {
          "type": "string",
          "enum": ["demolition", "rough-in", "structural", "mechanical", "electrical", "plumbing", "finish", "fixtures", "cleanup", "inspection", "trim", "paint", "framing", "milestone"],
          "description": "Work category"
        },
        "description": {
          "type": "string",
          "description": "Detailed description"
        },
        "notes": {
          "type": "string",
          "description": "Additional notes about the task"
        },
        "estimatedCost": {
          "type": "number",
          "description": "Estimated cost for this task"
        },
        "actualCost": {
          "type": "number",
          "description": "Actual cost incurred"
        },
        "location": {
          "type": "string",
          "description": "Specific location within project (e.g., 'north wall', 'island')"
        },
        "materialDependencies": {
          "type": "array",
          "items": { "$ref": "#/$defs/MaterialDependency" },
          "description": "Materials required for this task"
        },
        "subtasks": {
          "type": "array",
          "items": { "$ref": "#/$defs/Subtask" },
          "description": "Child tasks"
        }
      }
    },
    "Subtask": {
      "type": "object",
      "required": ["id", "name"],
      "description": "Child task within a parent task",
      "properties": {
        "id": {
          "type": "string",
          "pattern": "^[a-z0-9-]+$",
          "description": "Unique identifier (slug format)"
        },
        "name": {
          "type": "string",
          "description": "Display name"
        },
        "start": {
          "type": "string",
          "format": "date",
          "description": "Start date YYYY-MM-DD"
        },
        "end": {
          "type": "string",
          "format": "date",
          "description": "End date YYYY-MM-DD"
        },
        "status": {
          "type": "string",
          "enum": ["pending", "needs-scheduled", "scheduled", "confirmed", "in-progress", "completed", "blocked", "cancelled"],
          "description": "Current status"
        },
        "assignee": {
          "type": "string",
          "pattern": "^vendor:[a-z0-9-]+$",
          "description": "Tag reference to vendor"
        },
        "dependencies": {
          "type": "array",
          "items": { "type": "string" },
          "description": "Array of subtask IDs that must complete first"
        },
        "notes": {
          "type": "string",
          "description": "Additional notes"
        },
        "materialDependencies": {
          "type": "array",
          "items": { "$ref": "#/$defs/MaterialDependency" },
          "description": "Materials required for this subtask"
        }
      }
    },
    "MaterialDependency": {
      "type": "object",
      "required": ["id", "name", "status"],
      "properties": {
        "id": {
          "type": "string",
          "pattern": "^[a-z0-9-]+$",
          "description": "Unique identifier"
        },
        "name": {
          "type": "string",
          "description": "Material name"
        },
        "status": {
          "type": "string",
          "enum": ["need-to-select", "selected", "need-to-order", "ordered", "vendor-provided", "on-hand"],
          "description": "Material procurement status"
        },
        "vendor": {
          "type": "string",
          "pattern": "^vendor:[a-z0-9-]+$",
          "description": "Vendor supplying this material"
        },
        "expectedDate": {
          "type": "string",
          "format": "date",
          "description": "Expected delivery date"
        },
        "cost": {
          "type": "number",
          "description": "Material cost"
        },
        "quantity": {
          "type": "number",
          "minimum": 1,
          "description": "Number of items needed"
        },
        "notes": {
          "type": "string",
          "description": "Additional notes"
        },
        "detail": {
          "type": "string",
          "description": "Specification details (size, finish, model, etc.)"
        },
        "purchasingTask": {
          "type": "string",
          "description": "Reference to purchasing subtask ID if tracked there"
        }
      }
    },
    "LienWaiverEntry": {
      "type": "object",
      "description": "Individual lien waiver record",
      "properties": {
        "received": { "type": "boolean" },
        "date": { "type": "string", "format": "date" },
        "href": { "type": "string", "description": "Path to lien waiver document" }
      }
    },
    "Vendor": {
      "type": "object",
      "required": ["id", "name", "type"],
      "properties": {
        "id": {
          "type": "string",
          "pattern": "^[a-z0-9-]+$",
          "description": "Unique identifier"
        },
        "name": {
          "type": "string",
          "description": "Full vendor/company name"
        },
        "type": {
          "type": "string",
          "enum": [
            "general-contractor",
            "gc",
            "subcontractor",
            "specialty-contractor",
            "supplier",
            "materials",
            "equipment-rental",
            "utility",
            "government",
            "inspector",
            "designer",
            "consultant",
            "owner",
            "other"
          ],
          "description": "Type of vendor"
        },
        "trade": {
          "type": "string",
          "enum": [
            "general",
            "electrical",
            "plumbing",
            "hvac",
            "flooring",
            "drywall",
            "painting",
            "carpentry",
            "trim",
            "trim-paint",
            "cabinetry",
            "countertops",
            "tile",
            "roofing",
            "siding",
            "framing",
            "demolition",
            "hauling",
            "appliances",
            "fixtures",
            "stainless",
            "propane",
            "kitchen-equipment",
            "multiple"
          ],
          "description": "Primary trade or specialty"
        },
        "contact": {
          "type": "object",
          "properties": {
            "name": { "type": "string", "description": "Primary contact person" },
            "phone": { "type": "string" },
            "email": { "type": "string", "format": "email" },
            "address": {
              "type": "object",
              "properties": {
                "street": { "type": "string" },
                "city": { "type": "string" },
                "state": { "type": "string" },
                "zip": { "type": "string" }
              }
            }
          }
        },
        "license": {
          "type": "object",
          "properties": {
            "number": { "type": "string" },
            "state": { "type": "string" },
            "expiration": { "type": "string", "format": "date" }
          },
          "description": "Contractor license info"
        },
        "insurance": {
          "type": "object",
          "properties": {
            "liability": { "type": "boolean" },
            "workersComp": { "type": "boolean" },
            "policyNumber": { "type": "string" },
            "expiration": { "type": "string", "format": "date" }
          }
        },
        "rating": {
          "type": "number",
          "minimum": 1,
          "maximum": 5,
          "description": "Performance rating 1-5"
        },
        "status": {
          "type": "string",
          "enum": ["active", "inactive", "preferred", "blacklisted"],
          "description": "Vendor status"
        },
        "paymentTerms": {
          "type": "string",
          "enum": ["cod", "net-15", "net-30", "draws", "milestone"],
          "description": "Payment terms"
        },
        "folder": {
          "type": "string",
          "description": "Folder name in reference/receipts/"
        },
        "website": {
          "type": "string",
          "format": "uri"
        }
      }
    },
    "Receipt": {
      "type": "object",
      "required": ["id", "vendor", "date", "amount"],
      "properties": {
        "id": {
          "type": "string",
          "description": "Unique identifier"
        },
        "vendor": {
          "type": "string",
          "pattern": "^vendor:[a-z0-9-]+$",
          "description": "Tag reference to vendor"
        },
        "href": {
          "type": "string",
          "description": "Path to file relative to project root"
        },
        "date": {
          "type": "string",
          "format": "date",
          "description": "Receipt/invoice date"
        },
        "dueDate": {
          "type": "string",
          "format": "date",
          "description": "Payment due date"
        },
        "amount": {
          "type": "number",
          "description": "Total amount"
        },
        "tax": {
          "type": "number",
          "description": "Tax amount"
        },
        "description": {
          "type": "string",
          "description": "Description of charge"
        },
        "lineItems": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "description": { "type": "string" },
              "quantity": { "type": "number" },
              "unitPrice": { "type": "number" },
              "total": { "type": "number" }
            }
          }
        },
        "type": {
          "type": "string",
          "enum": ["estimate", "quote", "invoice", "receipt", "payment", "draw", "change-order", "credit"],
          "description": "Document type"
        },
        "category": {
          "type": "string",
          "enum": [
            "subcontractor",
            "materials",
            "labor",
            "professional",
            "permit",
            "site-support",
            "pm-fee",
            "lodging",
            "owner-furnished",
            "other"
          ],
          "description": "Expense category per contract Exhibit D"
        },
        "task": {
          "type": "string",
          "pattern": "^task:[a-z0-9-]+$",
          "description": "Tag reference to related task"
        },
        "changeOrder": {
          "type": "string",
          "pattern": "^co:[a-z0-9-]+$",
          "description": "Tag reference to change order if applicable"
        },
        "status": {
          "type": "string",
          "enum": ["draft", "pending", "approved", "paid", "disputed", "void"],
          "description": "Payment status"
        },
        "paidBy": {
          "type": "string",
          "enum": ["project", "owner"],
          "description": "Who paid: project (GC account) or owner (direct purchase)"
        },
        "markupPercent": {
          "type": "number",
          "minimum": 0,
          "maximum": 100,
          "description": "Markup percentage (default by category, 0 for owner-selected)"
        },
        "ownerSelected": {
          "type": "boolean",
          "description": "Owner selected this item - no markup but counts toward owed"
        },
        "allowable": {
          "type": "boolean",
          "default": true,
          "description": "True unless rework or excluded per contract"
        },
        "paymentMethod": {
          "type": "string",
          "enum": ["cash", "check", "credit-card", "debit", "wire", "ach", "venmo", "zelle"],
          "description": "How payment was made"
        },
        "checkNumber": {
          "type": "string",
          "description": "Check number if paid by check"
        },
        "invoiceNumber": {
          "type": "string",
          "description": "Vendor's invoice number"
        },
        "lienWaiver": {
          "type": "object",
          "properties": {
            "conditional": { "$ref": "#/$defs/LienWaiverEntry" },
            "unconditional": { "$ref": "#/$defs/LienWaiverEntry" }
          },
          "description": "Lien waiver status for this receipt"
        },
        "metadata": {
          "type": "object",
          "properties": {
            "ocrConfidence": { "type": "number", "minimum": 0, "maximum": 1 },
            "extractedAt": { "type": "string", "format": "date-time" },
            "extractedBy": { "type": "string" },
            "verified": { "type": "boolean" },
            "verifiedBy": { "type": "string" },
            "verifiedAt": { "type": "string", "format": "date-time" }
          },
          "description": "OCR and verification metadata"
        }
      }
    },
    "Note": {
      "type": "object",
      "required": ["id", "created", "content", "tags"],
      "properties": {
        "id": {
          "type": "string",
          "description": "Unique identifier"
        },
        "created": {
          "type": "string",
          "format": "date",
          "description": "Creation date YYYY-MM-DD"
        },
        "updated": {
          "type": "string",
          "format": "date",
          "description": "Last modified date YYYY-MM-DD"
        },
        "author": {
          "type": "string",
          "description": "Who wrote the note"
        },
        "content": {
          "type": "string",
          "description": "Note text (supports markdown)"
        },
        "tags": {
          "type": "array",
          "items": { "type": "string" },
          "description": "Array of entity references"
        },
        "type": {
          "type": "string",
          "enum": ["general", "issue", "decision", "question", "reminder", "change-request"],
          "description": "Note type"
        },
        "priority": {
          "type": "string",
          "enum": ["low", "normal", "high", "urgent"]
        },
        "pinned": {
          "type": "boolean",
          "description": "Pin to top of lists"
        },
        "resolved": {
          "type": "boolean",
          "description": "Whether issue/question is resolved"
        },
        "attachments": {
          "type": "array",
          "items": { "type": "string" },
          "description": "Paths to attached files"
        }
      }
    },
    "Milestone": {
      "type": "object",
      "required": ["id", "name", "date"],
      "properties": {
        "id": { "type": "string" },
        "name": { "type": "string" },
        "date": { "type": "string", "format": "date" },
        "description": { "type": "string" },
        "status": {
          "type": "string",
          "enum": ["upcoming", "reached", "missed"]
        },
        "tasks": {
          "type": "array",
          "items": { "type": "string" },
          "description": "Task IDs that must complete for this milestone"
        }
      }
    },
    "Budget": {
      "type": "object",
      "properties": {
        "total": {
          "type": "number",
          "description": "Total project budget"
        },
        "contingency": {
          "type": "number",
          "description": "Contingency amount"
        },
        "categories": {
          "type": "object",
          "additionalProperties": {
            "type": "object",
            "properties": {
              "budgeted": { "type": "number" },
              "spent": { "type": "number" }
            }
          }
        }
      }
    },
    "Payment": {
      "type": "object",
      "required": ["id", "name", "amount"],
      "properties": {
        "id": {
          "type": "string",
          "pattern": "^[a-z0-9-]+$",
          "description": "Unique identifier (e.g., 'deposit', 'draw-1', 'final')"
        },
        "name": {
          "type": "string",
          "description": "Display name (e.g., 'Deposit', 'Draw 1 - Rough-In')"
        },
        "amount": {
          "type": "number",
          "description": "Payment amount"
        },
        "scheduledDate": {
          "type": "string",
          "format": "date",
          "description": "Date payment is scheduled"
        },
        "receivedDate": {
          "type": "string",
          "format": "date",
          "description": "Date payment was received"
        },
        "milestone": {
          "type": "string",
          "pattern": "^milestone:[a-z0-9-]+$",
          "description": "Tag reference to milestone that triggers this payment"
        },
        "status": {
          "type": "string",
          "enum": ["scheduled", "awaiting-request", "requested", "due", "received", "deferred", "overdue"],
          "description": "Payment status: scheduled (future), awaiting-request (milestone reached, no draw request), requested (GC submitted), due (approved for payment), received (paid), deferred (postponed), overdue (past due)"
        },
        "method": {
          "type": "string",
          "enum": ["cash", "check", "credit-card", "debit", "wire", "ach", "venmo", "zelle"],
          "description": "Payment method"
        },
        "checkNumber": {
          "type": "string",
          "description": "Check number if paid by check"
        },
        "href": {
          "type": "string",
          "description": "Path to payment receipt/confirmation"
        },
        "lienWaivers": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "vendor": {
                "type": "string",
                "pattern": "^vendor:[a-z0-9-]+$",
                "description": "Vendor providing lien waiver"
              },
              "type": {
                "type": "string",
                "enum": ["conditional", "unconditional"]
              },
              "received": { "type": "boolean" },
              "date": { "type": "string", "format": "date" },
              "href": { "type": "string" }
            }
          },
          "description": "Lien waivers collected for this draw"
        },
        "notes": {
          "type": "string",
          "description": "Additional notes about this payment"
        }
      }
    },
    "ChangeOrder": {
      "type": "object",
      "required": ["id", "number", "requestDate", "description", "costImpact", "status"],
      "properties": {
        "id": {
          "type": "string",
          "pattern": "^[a-z0-9-]+$",
          "description": "Unique identifier"
        },
        "number": {
          "type": "integer",
          "minimum": 1,
          "description": "Sequential change order number (CO-001, CO-002, etc.)"
        },
        "requestDate": {
          "type": "string",
          "format": "date",
          "description": "Date change order was requested"
        },
        "description": {
          "type": "string",
          "description": "Description of the change"
        },
        "reason": {
          "type": "string",
          "enum": [
            "owner-request",
            "unforeseen-condition",
            "design-change",
            "code-compliance",
            "value-engineering",
            "scope-clarification",
            "other"
          ],
          "description": "Reason for change order"
        },
        "costImpact": {
          "type": "number",
          "description": "Cost impact (positive = increase, negative = credit)"
        },
        "scheduleImpact": {
          "type": "integer",
          "description": "Schedule impact in days (positive = delay)"
        },
        "tasks": {
          "type": "array",
          "items": { "type": "string" },
          "description": "Task IDs affected by this change order"
        },
        "receipts": {
          "type": "array",
          "items": { "type": "string" },
          "description": "Receipt IDs associated with this change order"
        },
        "approvals": {
          "type": "array",
          "items": {
            "type": "object",
            "required": ["party", "status"],
            "properties": {
              "party": {
                "type": "string",
                "enum": ["owner", "gc", "sub"],
                "description": "Approving party"
              },
              "vendor": {
                "type": "string",
                "pattern": "^vendor:[a-z0-9-]+$",
                "description": "Vendor reference for sub approvals"
              },
              "status": {
                "type": "string",
                "enum": ["pending", "approved", "rejected"],
                "description": "Approval status"
              },
              "date": {
                "type": "string",
                "format": "date",
                "description": "Date of approval/rejection"
              },
              "signedBy": {
                "type": "string",
                "description": "Name of person who signed"
              },
              "href": {
                "type": "string",
                "description": "Path to signed approval document"
              },
              "notes": {
                "type": "string",
                "description": "Comments from approving party"
              }
            }
          },
          "description": "Multi-party approval chain"
        },
        "status": {
          "type": "string",
          "enum": ["draft", "pending", "approved", "rejected", "void"],
          "description": "Overall change order status"
        },
        "href": {
          "type": "string",
          "description": "Path to change order document"
        }
      }
    },
    "Contract": {
      "type": "object",
      "description": "Contract terms and financial structure",
      "properties": {
        "type": {
          "type": "string",
          "enum": ["fixed-price", "cost-plus", "time-materials", "unit-price"],
          "description": "Contract type"
        },
        "targetPrice": {
          "type": "number",
          "description": "Target price (cost-plus contracts)"
        },
        "contingencyPercent": {
          "type": "number",
          "description": "Contingency percentage"
        },
        "contingencyAmount": {
          "type": "number",
          "description": "Contingency amount"
        },
        "markupRules": {
          "type": "object",
          "additionalProperties": {
            "type": "number",
            "minimum": 0,
            "maximum": 100
          },
          "description": "Markup percentage by category (e.g., { 'subcontractor': 20, 'permits': 0 })"
        },
        "parties": {
          "type": "object",
          "properties": {
            "owner": {
              "type": "string",
              "pattern": "^vendor:[a-z0-9-]+$",
              "description": "Owner vendor reference"
            },
            "contractor": {
              "type": "string",
              "pattern": "^vendor:[a-z0-9-]+$",
              "description": "General contractor vendor reference"
            }
          }
        },
        "startDate": {
          "type": "string",
          "format": "date"
        },
        "completionDate": {
          "type": "string",
          "format": "date"
        },
        "href": {
          "type": "string",
          "description": "Path to signed contract document"
        }
      }
    },
    "Issue": {
      "type": "object",
      "required": ["id", "created", "assignee", "status"],
      "description": "Unified issue - includes questions, lifecycle issues, and auto-detected issues",
      "properties": {
        "id": {
          "type": "string",
          "description": "Unique identifier (pattern: i-{type}-{slug}, il-{type}-{task}, id-{type}-{task})"
        },
        "created": {
          "type": "string",
          "format": "date",
          "description": "Creation date YYYY-MM-DD"
        },
        "category": {
          "type": "string",
          "enum": ["ASSIGN", "SCHEDULE", "ORDER", "SPECIFY", "TRACK", "DECIDE"],
          "description": "Action category - what action is needed?"
        },
        "type": {
          "type": "string",
          "enum": [
            "assignee", "date", "date-range", "dependency", "yes-no", "select-one",
            "material-status", "notification", "free-text",
            "schedule-conflict", "missing-assignee", "past-due", "unscheduled-blocker", "material-overdue"
          ],
          "description": "Structured issue type"
        },
        "source": {
          "type": "string",
          "enum": ["manual", "auto-lifecycle", "auto-detection"],
          "description": "Where this issue came from"
        },
        "title": {
          "type": "string",
          "description": "Short summary for display"
        },
        "prompt": {
          "type": "string",
          "description": "Question text for structured questions"
        },
        "question": {
          "type": "string",
          "description": "Legacy field - use prompt instead"
        },
        "description": {
          "type": "string",
          "description": "Additional context"
        },
        "config": {
          "type": "object",
          "description": "Type-specific options (tradeFilter, options, statusOptions)"
        },
        "relatedTask": {
          "type": "string",
          "description": "Task ID (without task: prefix)"
        },
        "relatedTasks": {
          "type": "array",
          "items": { "type": "string" },
          "description": "For cross-task issues (schedule conflicts)"
        },
        "relatedMaterial": {
          "type": "string",
          "description": "Material ID from task's materialDependencies"
        },
        "assignee": {
          "type": "string",
          "enum": ["brandon", "dave", "tonia", "system"],
          "description": "Who should answer this issue"
        },
        "status": {
          "type": "string",
          "enum": ["open", "answered", "resolved", "dismissed"],
          "description": "Issue status"
        },
        "priority": {
          "type": "string",
          "enum": ["low", "normal", "high", "critical"],
          "description": "Issue priority level"
        },
        "response": {
          "description": "Structured response or legacy string"
        },
        "responseNotes": {
          "type": "string",
          "description": "Optional explanation with response"
        },
        "respondedAt": {
          "type": "string",
          "format": "date",
          "description": "Date response was added YYYY-MM-DD"
        },
        "reviewStatus": {
          "type": "string",
          "enum": ["pending", "accepted", "rejected"],
          "description": "Review workflow status"
        },
        "rejectionReason": {
          "type": "string",
          "description": "Reason if rejected"
        },
        "appliedChanges": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "entity": { "type": "string", "enum": ["task", "subtask", "material"] },
              "entityId": { "type": "string" },
              "field": { "type": "string" },
              "oldValue": {},
              "newValue": {}
            }
          },
          "description": "Audit trail of changes made"
        },
        "resolvedAt": {
          "type": "string",
          "format": "date",
          "description": "Date resolved YYYY-MM-DD"
        },
        "resolvedDate": {
          "type": "string",
          "format": "date",
          "description": "Legacy field - use resolvedAt instead"
        },
        "resolvedBy": {
          "type": "string",
          "enum": ["manual", "auto"],
          "description": "How this was resolved"
        },
        "detectionRule": {
          "type": "string",
          "description": "Rule that created this issue (for auto-detection)"
        },
        "lastChecked": {
          "type": "string",
          "format": "date",
          "description": "When condition was last verified (for auto-detection)"
        }
      }
    }
  }
}
